# Object-Oriented-C-

// Accept N numbers from user and display all such elements which are
// even and divisible by 5.
// Input : N : 6
// Elements : 85 66 3 80 93 88
// Output : 80

#include<stdio.h>
#include<stdlib.h>

void EvnDivNum(int *Arr,int iValue)
{
	int i=0;
	int iAns=0;

	for(i=0;i<iValue;i++)
	{
		
		if((Arr[i]%2==0)&&(Arr[i]%5==0))
		{
			printf("%d",Arr[i]);
		}
 
	}
}

int main()
{
	int iValue=0;

	int *Arr=NULL;
	int i=0;

	int iRet=0;

	printf("Enter value");
	scanf("%d",&iValue);

   Arr=(int*)malloc(sizeof(int)*iValue);

   printf("Enter numbers in the Array");
   for(i=0;i<iValue;i++)
   {
   	scanf("%d",&Arr[i]);
   }

   EvnDivNum(Arr,iValue);
   
   free(Arr);

   return 0;
}
