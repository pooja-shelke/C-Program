# Object-Oriented-C-

//  Accept N numbers from user and accept one another number as NO ,
// return frequency of NO form it.

// Input : N : 6
// NO: 66
// Elements : 85 66 3 66 93 88
// Output : 2
// Input : N : 6
// NO: 12
// Elements : 85 11 3 15 11 111
// Output : 0

#include<stdio.h>
#include<stdlib.h>

int CntFrqPntNum(int *Arr,int iValue1,int iValue2)
{
    int i=0;
    int iCnt=0;

    for(i=0;i<iValue1;i++)
    {
    	if(Arr[i]==iValue2)
    	{
    		iCnt++;
    	}
    }
    return iCnt;
}
int main()
{
	int iValue1=0;
	int iValue2=0;
    int i=0;
    int iRet=0;

	int *Arr=NULL;

	printf("Enter iValue");
	scanf("%d",&iValue1);

	Arr=(int*)malloc(sizeof(int)*iValue1);

	printf("Enter elements");
	for(i=0;i<iValue1;i++)
	{
		scanf("%d",&Arr[i]);
	}
    
	printf("Enter Number that You want to search");
	scanf("%d",&iValue2);

    iRet=CntFrqPntNum(Arr,iValue1,iValue2);

    printf("%d",iRet);

    free(Arr);

    return 0;
}
